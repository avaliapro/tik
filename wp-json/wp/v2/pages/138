px) {\r\n            .toggle-button {\r\n                font-size: 14px;\r\n                padding: 10px 20px;\r\n                max-width: 100%; \/* Permite que os bot\u00f5es ocupem toda a largura dispon\u00edvel em telas pequenas *\/\r\n            }\r\n        }\r\n    <\/style>\r\n<\/head>\r\n<body>\r\n    <div class=\"container\">\r\n        <button class=\"toggle-button\">R$1,5<\/button>\r\n        <button class=\"toggle-button\">R$5<\/button>\r\n        <button class=\"toggle-button\">R$10<\/button>\r\n        <button class=\"toggle-button\">R$559,51<\/button>\r\n    <\/div>\r\n\r\n    <script>\r\n        \/\/ Quando o DOM estiver carregado\r\n        document.addEventListener(\"DOMContentLoaded\", () => {\r\n            const buttons = document.querySelectorAll(\".toggle-button\");\r\n\r\n            \/\/ Fun\u00e7\u00e3o para gerenciar a sele\u00e7\u00e3o e altera\u00e7\u00e3o de estilo\r\n            buttons.forEach(button => {\r\n                button.addEventListener(\"click\", () => {\r\n                    \/\/ Se o bot\u00e3o n\u00e3o estiver selecionado\r\n                    if (!button.classList.contains(\"selected\")) {\r\n                        \/\/ Remover sele\u00e7\u00e3o de todos os bot\u00f5es\r\n                        buttons.forEach(btn => {\r\n                            btn.classList.remove(\"selected\");\r\n                            btn.style.backgroundColor = '#F6F2F6'; \/\/ Resetando a cor de fundo para #F6F2F6\r\n                            btn.style.color = '#000000'; \/\/ Resetando a cor do texto para #000000\r\n                        });\r\n                        \r\n                        \/\/ Mudar a cor de fundo do bot\u00e3o clicado para #FFF3F8\r\n                        button.style.backgroundColor = '#FFF3F8';\r\n                        \r\n                        \/\/ Mudar a cor do texto do bot\u00e3o para #FF3661\r\n                        button.style.color = '#FF3661';\r\n                        \r\n                        \/\/ Adicionar sele\u00e7\u00e3o ao bot\u00e3o clicado\r\n                        button.classList.add(\"selected\");\r\n                    } else {\r\n                        \/\/ Se o bot\u00e3o j\u00e1 estiver selecionado, remova a sele\u00e7\u00e3o\r\n                        button.classList.remove(\"selected\");\r\n\r\n                        \/\/ Alterar a cor de fundo e do texto de volta\r\n                        button.style.backgroundColor = '#F6F2F6'; \/\/ Retorna \u00e0 cor original\r\n                        button.style.color = '#000000'; \/\/ Retorna \u00e0 cor original do texto\r\n                    }\r\n                });\r\n            });\r\n        });\r\n    <\/script>\r\n<\/body>\r\n<\/html>\r\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<div class=\"elementor-element elementor-element-6156005 elementor-button-align-stretch elementor-widget elementor-widget-form\" data-id=\"6156005\" data-element_type=\"widget\" data-settings=\"{&quot;step_next_label&quot;:&quot;Next&quot;,&quot;step_previous_label&quot;:&quot;Previous&quot;,&quot;button_width&quot;:&quot;100&quot;,&quot;step_type&quot;:&quot;number_text&quot;,&quot;step_icon_shape&quot;:&quot;circle&quot;}\" data-widget_type=\"form.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t\t\t\t\t<form class=\"elementor-form\" method=\"post\" name=\"New Form\">\n\t\t\t<input type=\"hidden\" name=\"post_id\" value=\"138\"\/>\n\t\t\t<input type=\"hidden\" name=\"form_id\" value=\"6156005\"\/>\n\t\t\t<input type=\"hidden\" name=\"referer_title\" value=\"Saque\" \/>\n\n\t\t\t\t\t\t\t<input type=\"hidden\" name=\"queried_id\" value=\"138\"\/>\n\t\t\t\n\t\t\t<div class=\"elementor-form-fields-wrapper elementor-labels-\">\n\t\t\t\t\t\t\t\t<div class=\"elementor-field-type-select elementor-field-group elementor-column elementor-field-group-name elementor-col-100\">\n\t\t\t\t\t\t\t\t\t\t\t\t<label for=\"form-field-name\" class=\"elementor-field-label elementor-screen-only\